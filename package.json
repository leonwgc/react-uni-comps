    "@typescript-eslint/parser": "^5.6.0",
    "animate.css": "^4.1.1",
    "babel-eslint": "^10.1.0",
    "dayjs": "^1.10.7",
    "dumi": "^1.1.35",
    "dumi-theme-mobile": "^1.1.18",
    "ejs": "^3.1.6",
    "eslint": "^8.4.1",
    "eslint-plugin-react": "^7.27.1",
