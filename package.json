    "clsx": "^1.1.1",
    "color": "^4.1.0",
    "copy-text-to-clipboard": "^3.0.1",
    "intersection-observer": "^0.12.0",
    "nanoid": "^3.1.30",
    "react-is": "^17.0.2",
    "react-transition-group": "^4.4.2",
    "signature_pad": "^4.0.0",
    "styled-components": "^5.3.3"
  },
  "sideEffects": [
    "*.less"
  ]
}
