{
  "name": "react-uni-comps",
  "version": "1.8.3.beta1",
  "description": "react组件库, desktop & mobile react components via hooks",
  "homepage": "https://leonwgc.github.io/react-uni-comps/",
  "keywords": [
    "react",
    "component",
    "hooks"
