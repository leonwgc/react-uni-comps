    "babel-plugin-styled-components": "^2.0.6",
    "dayjs": "^1.10.7",
    "dumi": "^1.1.38",
    "dumi-theme-mobile": "^1.1.21",
    "ejs": "^3.1.6",
    "eslint": "^8.6.0",
    "eslint-plugin-react": "^7.27.1",
