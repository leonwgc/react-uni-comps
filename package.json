    "husky": "^7.0.4",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-preview": "^0.2.5",
    "jest-watch-typeahead": "^1.1.0",
    "lint-staged": "^12.1.2",
    "npm-run-all": "^4.1.5",
    "packw": "1.2.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2",
    "simple-redux-store": "^1.0.2",
    "ts-jest": "^28.0.3",
    "typer-js": "^5.5.3",
    "typescript": "^4.7.4",
    "vconsole": "^3.11.0",
    "yargs": "^17.3.0"
