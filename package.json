  "files": [
    "dist",
    "es",
    "types"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,less}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@react-spring/web": "^9.4.2",
